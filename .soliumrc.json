{
  "extends": "solium:all",
  "plugins": ["security"],
  "rules": {
    "quotes": ["error", "double"],
    "indentation": ["error", "tab"],
    "max-len": ["warning", 79],
    "no-constant": ["error"],
    "security/enforce-explicit-visibility": ["error"],
    "security/no-block-members": ["warning"],
    "security/no-inline-assembly": ["warning"],
    "security/no-sha3": ["error"],
    "security/no-unreachable-code": ["warning"]
  }
}